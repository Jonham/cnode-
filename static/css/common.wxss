@import "./animation.wxss";

.dis-flex{
    display: flex;
    display: -webkit-flex;
}
.flex-justify{
    justify-content:space-between;
    -webkit-justify-content:space-between;
}

.flex-items-center {
  align-items: center;
}
.dot{
    width: 8rpx;
    height: 8rpx;
    border-radius: 50%;
    background: #888;
    display: inline-block;
    margin:6rpx 12rpx;
}
.is-top{
    background:#80bd01;
    color:#fff;
}
.tab{
    background:#e5e5e5;
    color:#999;
}
.tips{
    height:40rpx;
    line-height: 40rpx;
    padding:0 10rpx;
    border-radius:8rpx;
}
.good{
    position: absolute;
    top: 40rpx;
    right: 20rpx;
    width: 100rpx;
    height: 54rpx;
    line-height: 54rpx;
    color: red;
    border: 2px solid red;
    text-align: center;
    border-radius: 8rpx;
    font-size: 32rpx; 
    opacity: 0.3;
    transform: rotate(30deg);
}

.close{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    right: 20rpx;
    top: 20rpx;
}

.diy-navigator-hover{
    background: rgb(247, 247, 247);
    position: relative;
    z-index: 1;
}
.diy-navigator-hover::after{
    content: '';
    position: absolute;
    top: 50%;
    left: 50%;
    z-index: 0;
    width: 20rpx;
    height: 20rpx;
    background:rgba(179, 179, 179, 0.1);
    border-radius: 50%;
    animation: navHover 0.8s;
}

